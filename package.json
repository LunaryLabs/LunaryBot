{
  "name": "lunary-bot",
  "version": "0.0.2",
  "type": "module",
  "main": "dist/index.js",
  "packageManager": "pnpm@7.26.3",
  "license": "MIT",
  "scripts": {
    "prod:start": "NODE_ENV=semi-production tsx src/Loader.ts",
    "dev:start": "NODE_ENV=development tsx watch src/Loader.ts",

    "dist:start": "NODE_ENV=production node dist/Loader.js",
    "dist:stand": "NODEw_ENV=production node dist/Loader.js",
    "dist:build": "tsc && tsc-alias",

    "prisma:generate": "prisma generate --schema=./src/database/schema.prisma",
    "prisma:migrate": "prisma migrate deploy --schema=./src/database/schema.prisma"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@prisma/client": "^4.9.0",
    "@sentry/node": "^7.34.0",
    "@sentry/tracing": "^7.34.0",
    "bufferutil": "^4.0.7",
    "discord.js": "^14.7.1",
    "discordx": "^11.6.0",
    "erlpack": "github:discord/erlpack",
    "pino": "^8.8.0",
    "pino-pretty": "^11.0.0",
    "utf-8-validate": "^6.0.2",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "dotenv": "^16.0.3",
    "prisma": "^4.9.0",
    "tsc-alias": "^1.8.2",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  }
}
